{
  "name": "@funkp/core",
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/main.cjs.js",
  "module": "./dist/main.es.js",
  "types": "./dist/main.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/main.ts",
      "types": "./dist/main.d.ts",
      "import": "./dist/main.es.js",
      "default": "./dist/main.cjs.js"
    },
    "./functions": {
      "development": "./src/functions/index.ts",
      "types": "./dist/functions/index.d.ts",
      "import": "./dist/functions.es.js",
      "default": "./dist/functions.cjs.js"
    },
    "./Either": {
      "development": "./src/Either/index.ts",
      "types": "./dist/Either/index.d.ts",
      "import": "./dist/Either.es.js",
      "default": "./dist/Either.cjs.js"
    },
    "./Option": {
      "development": "./src/Option/index.ts",
      "types": "./dist/Option/index.d.ts",
      "import": "./dist/Option.es.js",
      "default": "./dist/Option.cjs.js"
    },
    "./Effect": {
      "development": "./src/Effect/index.ts",
      "types": "./dist/Effect/index.d.ts",
      "import": "./dist/Effect.es.js",
      "default": "./dist/Effect.cjs.js"
    },
    "./Brand": {
      "development": "./src/Brand/index.ts",
      "types": "./dist/Brand/index.d.ts",
      "import": "./dist/Brand.es.js",
      "default": "./dist/Brand.cjs.js"
    },
    "./Fiber": {
      "development": "./src/Fiber/index.ts",
      "types": "./dist/Fiber/index.d.ts",
      "import": "./dist/Fiber.es.js",
      "default": "./dist/Fiber.cjs.js"
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "devDependencies": {
    "vitest": "^3",
    "@funkp/vitest": "0.0.1"
  },
  "nx": {
    "targets": {
      "build": {
        "executor": "@nx/vite:build",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "platform": "node",
          "outputPath": "libs/core/dist",
          "main": "libs/core/src/main.ts",
          "tsConfig": "libs/core/tsconfig.build.json",
          "format": [
            "esm",
            "cjs"
          ],
          "esbuildOptions": {
            "sourcemap": true,
            "outExtension": {
              ".js": ".mjs",
              ".cjs": ".cjs"
            }
          },
          "declarationRootDir": "libs/core/src"
        }
      },
      "test": {
        "executor": "@nx/vite:test",
        "outputs": [
          "{workspaceRoot}/coverage/libs/core"
        ],
        "options": {
          "config": "libs/core/vite.config.ts"
        }
      }
    }
  },
  "dependencies": {
    "@funkp/vitest": "0.0.1"
  }
}
